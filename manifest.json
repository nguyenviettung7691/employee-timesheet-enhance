{
    "manifest_version": 3,
    "name": "Employee Timesheet Enhance",
    "version": "2.010825",
    "permissions": [ "activeTab", "scripting" ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://employee.bravosuite.io/time-sheet"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }